{
  "name": "zreact",
  "version": "2.0.0",
  "description": "React like,copy by preact",
  "main": "dist/zreact.js",
  "module": "dist/zreact.esm.js",
  "jsnext:main": "dist/zreact.esm.js",
  "minified:main": "dist/zreact.min.js",
  "typings": "types/zreact.d.ts",
  "repository": "https://github.com/zeromake/zreact",
  "author": "zeromake <a390720046@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeromake/zreact/issues"
  },
  "homepage": "https://github.com/zeromake/zreact",
  "scripts": {
    "clean": "rimraf dist",
    "build:all": "npm run clean && npm run build:min && npm run build",
    "build": "cross-env NODE_ENV=dev rollup -c scripts/rollup.config.js",
    "build:devtools": "cross-env NODE_ENV=dev rollup -c scripts/rollup.config.devtools.js",
    "build:min": "cross-env NODE_ENV=production rollup -c scripts/rollup.config.js",
    "dev": "cross-env NODE_ENV=dev rollup -c scripts/rollup.config.js -w",
    "coverage": "cat ./test/coverage/lcov.info | coveralls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublishOnly": "npm run build:all",
    "docs": "rimraf docs && typedoc --out docs src/index.ts",
    "test": "jest -u --coverage -c scripts/jest/jest.config.js"
  },
  "keywords": [
    "zreact",
    "react",
    "virtual dom",
    "vdom",
    "components",
    "virtual",
    "dom"
  ],
  "files": [
    "src",
    "dist",
    "types",
    "lib",
    "typings.json",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "conventional-changelog-cli": "^2.0.11",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "gzip-size-cli": "^3.0.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.68.2",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-license": "0.7.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-typescript": "^1.2.0",
    "rollup-watch": "^4.3.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.2"
  }
}
